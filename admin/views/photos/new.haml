- # encoding: utf-8
%ul.nav.nav-tabs
  %li=link_to tag_icon(:list, pat(:list)), url(:photos, :index)
  %li.active=link_to tag_icon(:plus, pat(:new)), url(:photos, :new)

.tabs-content
  -form_for :photo, url(:photos, :create), multipart: true, :class => 'form-horizontal' do |f|
    =partial 'photos/form', :locals => { :f => f }
